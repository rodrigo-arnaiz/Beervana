{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.2"
        }
    },
    "routes": [
        {
            "src": "/build/assets/(.*)",
            "dest": "/build/assets/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
            {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
        {
            "src": "/(.*\\.php)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}